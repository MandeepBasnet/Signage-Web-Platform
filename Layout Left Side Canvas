                                                                                                                                                                                                                                                                                                                                                                            p.cycle-slide {
                                                                                                                                                                                                                                                                                                                                                                              margin-bottom: 0;
                                                                                                                                                                                                                                                                                                                                                                            }
                                                                                                                                                                                                                                                                                                                                                                          </style>
                                                                                                                                                                                                                                                                                                                                                                          <style
                                                                                                                                                                                                                                                                                                                                                                            type="text/css">
                                                                                                                                                                                                                                                                                                                                                                            /* Template Styles */
                                                                                                                                                                                                                                                                                                                                                                            table.DataSetTable {
                                                                                                                                                                                                                                                                                                                                                                              font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
                                                                                                                                                                                                                                                                                                                                                                              font-size: 14px;
                                                                                                                                                                                                                                                                                                                                                                              width: 100%;
                                                                                                                                                                                                                                                                                                                                                                              border-collapse: collapse;
                                                                                                                                                                                                                                                                                                                                                                              table-layout: fixed;
                                                                                                                                                                                                                                                                                                                                                                            }

                                                                                                                                                                                                                                                                                                                                                                            tr.HeaderRow {
                                                                                                                                                                                                                                                                                                                                                                              font-size: 1.1em;
                                                                                                                                                                                                                                                                                                                                                                              text-align: center;
                                                                                                                                                                                                                                                                                                                                                                              padding-top: 5px;
                                                                                                                                                                                                                                                                                                                                                                              padding-bottom: 4px;
                                                                                                                                                                                                                                                                                                                                                                              background-color: #A7C942;
                                                                                                                                                                                                                                                                                                                                                                              color: #ffffff;
                                                                                                                                                                                                                                                                                                                                                                            }

                                                                                                                                                                                                                                                                                                                                                                            tr#row_1 {

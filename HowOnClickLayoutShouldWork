if (Array.isArray(items)) {\n        items.reverse();\n    } else {\n        \/\/ If it's an object, reverse the keys\n        var newItems = {};\n        Object.keys(items).reverse().forEach(function(key) {\n            newItems[key] = items[key];\n        });\n        items = $(newItems);\n    }\n}\nreturn {dataItems: items};",
